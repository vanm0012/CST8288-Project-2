<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <session-config>
        <session-timeout>
            1
        </session-timeout>
    </session-config>

    <servlet>
        <servlet-name>SymphonyServlet</servlet-name>
        <servlet-class>io.github.vanm0012.symphonymanager.servlet.SymphonySV</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SymphonyServlet</servlet-name>
        <url-pattern>/symphony</url-pattern>
    </servlet-mapping>

</web-app>